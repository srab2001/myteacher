{
  "version": 2,
  "installCommand": "npm install --ignore-scripts && npm run postinstall",
  "buildCommand": "npm run build",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}
