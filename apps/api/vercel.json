{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/.pnpm/**/.prisma/**",
          "node_modules/.pnpm/**/libquery_engine*",
          "node_modules/@prisma/**",
          "node_modules/.prisma/**",
          "prisma/schema.prisma"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
