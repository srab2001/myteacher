{
  "version": 2,
  "buildCommand": "npx prisma@5.22.0 generate && npx prisma@5.22.0 db push --skip-generate && npx tsx prisma/seed-if-empty.ts",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/generated/**",
          "prisma/schema.prisma"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
